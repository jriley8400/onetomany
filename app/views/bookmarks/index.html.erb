<h1>Bookmarks</h1>

<p><a href="/new_bookmark">Add a New Bookmark</a></p>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Url</th>
      <th>Description</th>
      <th>User</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
  <% @bookmarks.each do |bookmark| %>
    <tr>
          <td><%= bookmark.name %></td>
          <td><%= bookmark.url %></td>
          <td><%= bookmark.description %></td>
          <td><%= bookmark.user_id %></td>
          <td><button><a href="/bookmarks/<%= bookmark.id %>">Show</a></button></td>
          <td><button><a href="/bookmarks/<%= bookmark.id %>/edit">Edit</a></button></td>
          <td><button><a href="/bookmarks/<%= bookmark.id %>/destroy">Destroy</a></button></td>
    </tr>
  <% end %>
  </tbody>
</table>

